    <UserSecretsId>4742d1d9-24f6-4445-8207-e4a6cd50fd3b</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Sdk.Functions" Version="3.0.3" />
